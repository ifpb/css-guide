@startmindmap
<style>
mindmapDiagram {
  Node {
    BackGroundColor white
  }
  rootNode {
    FontSize 35
  }
  :depth(1) {
    BackGroundColor LightYellow
    FontSize 15
  }
  leafNode {
    FontColor gray
    FontSize 15
  }
  .containerStyle * {
    BackgroundColor YellowGreen
  }
  .containerBoxAlignStyle * {
    BackgroundColor YellowGreen
    LineStyle 4
    LineThickness .5
  }
  .itemStyle * {
    BackgroundColor SkyBlue
  }
  .itemBoxAlignStyle * {
    BackgroundColor SkyBlue
    LineStyle 4
    LineThickness .5
  }
}
</style>
